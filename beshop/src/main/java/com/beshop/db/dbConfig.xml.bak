<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="com/beshop/db/bit-properties"></properties>
<typeAliases>
	<typeAlias type="com.beshop.vo.Be_NoticeVo" alias="Be_NoticeVo"/>
	<typeAlias type="com.beshop.vo.Be_QnaVo" alias="Be_QnaVo"/>
	<typeAlias type="com.beshop.vo.ChargePointVo" alias="Be_ChargeVo"/>
  	<typeAlias type="com.beshop.vo.PayPointVo" alias="Be_pPayVo"/>
  	<typeAlias type="com.beshop.vo.PointBalanceVo" alias="Be_BalanceVo"/>
  	 <typeAlias type="com.beshop.vo.UserVo" alias="uservo"/>
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${user}"/>
        <property name="password" value="${pwd}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/beshop/db/be_noticeMapper.xml"/>
    <mapper resource="com/beshop/db/be_qnaMapper.xml"/>
    <mapper resource="com/beshop/db/be_pointMapper.xml"/>
  </mappers>
</configuration>